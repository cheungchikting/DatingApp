    <div class="content">
      <form action="/editfilter" method="POST">
        <div class="user-details">
          <div class="input-box">
            <label for="gender">Gender</label>
              <select name="preferredGender" id="gender" required>
                {{#if data.preferredGender}}
                <option value="{{data.preferredGender}}" selected>{{data.preferredGender}}</option >
                {{else}}
              <option value="" class="select">-Select-</option>
                {{/if}}
                <option value="Male">Male</option>
                <option value="Female">Female</option>
              </select>
          </div>
          <div class="input-box row">
              <div clas="col-12">
                <label for="age" id="label_age">Age</label>
              </div>

              <select name="min_age" id="min_age" required>
              {{#if data.min_age}}
              <option value="{{data.min_age}}" selected>{{data.min_age}}</option >
              {{else}}
           <option value="" class="select">From</option>
              {{/if}}
              </select>

              <select name="max_age" id="max_age" required>
              {{#if data.max_age}}
              <option value="{{data.max_age}}" selected>{{data.max_age}}</option >
              {{else}}
              <option value="" class="select">To</option>
              {{/if}}
              </select>
          </div>

          <div class="input-box row">
              <div clas="col-12">
                <label for="height">Height</label>
              </div>

              <select name="min_height" id="min_height" required>
              {{#if data.min_height}}
              <option value="{{data.min_height}}" selected>{{data.min_height}}</option>
              {{else}}
              <option value="" class="select">Form</option>
              {{/if}}
              </select>

              <select name="max_height" id="max_height" required>
              {{#if data.max_height}}
              <option value="{{data.max_height}}" selected>{{data.max_height}}</option>
              {{else}}
              <option value="" class="select">To</option>
              {{/if}}
              </select>
          </div> 
            

            <div class="input-box">
              <label for="education">Education</label>
              <select name="preferredEducation" id="education" required>
              {{#if data.preferredEducation}}
              <option value="{{data.preferredEducation}}" selected>{{data.preferredEducationName}}</option>
              {{else}}
              <option value="" class="select">-Select-</option>
              {{/if}}
              </select>
            </div>


