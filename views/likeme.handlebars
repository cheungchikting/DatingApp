                          <span style="font-size: 30px; ">
                          <i class="far fa-heart"></i>
                          </span>
                        </button>
                      
                        <button class="dislikeBtn" data-id="{{id}}">
                          <span style="font-size: 30px; color: white;">
                          <i class="fas fa-times"></i>
                          </span>
                        </button>
                

                      </div>
                    </div>
                    {{/each}}
