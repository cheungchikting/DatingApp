    <div class="row">
        <div class="col-11 random-card">
            <div class="box-container">
                <div class="container">
                    {{#each data}}
                    <div class="box">
                      <a href="/profiles/{{id}}">
                      <div id="carouselExampleIndicators{{id}}" class="carousel slide" data-bs-interval="false">
                        <div class="carousel-indicators">
                            {{#if photos.pc1}}
                               <button type="button" data-bs-target="#carouselExampleIndicators{{id}}" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
                            {{/if}}
                            {{#if photos.pc2}}
                               <button type="button" data-bs-target="#carouselExampleIndicators{{id}}" data-bs-slide-to="1"  aria-label="Slide 2"></button>
                            {{/if}}
                            {{#if photos.pc3}}
                               <button type="button" data-bs-target="#carouselExampleIndicators{{id}}" data-bs-slide-to="2" aria-label="Slide 3"></button>
                            {{/if}}
                            {{#if photos.pc4}}
                               <button type="button" data-bs-target="#carouselExampleIndicators{{id}}" data-bs-slide-to="3" aria-label="Slide 4"></button>
                            {{/if}}
                            {{#if photos.pc5}}
                               <button type="button" data-bs-target="#carouselExampleIndicators{{id}}" data-bs-slide-to="4" aria-label="Slide 5"></button>
                            {{/if}}
                            {{#if photos.pc6}}
                               <button type="button" data-bs-target="#carouselExampleIndicators{{id}}" data-bs-slide-to="5" aria-label="Slide 6"></button>
                            {{/if}}
                        </div>
                        <div class="carousel-inner">
                             {{#if photos.pc1}}
                          <div class="carousel-item active img-fluid">
                               <img src="/upload/{{photos.pc1}}" alt="1">
                          </div>
                            {{/if}}

                            {{#if photos.pc2}}
                          <div class="carousel-item  img-fluid">
                               <img src="/upload/{{photos.pc2}}" alt="2">
                          </div>
                            {{/if}}

                            {{#if photos.pc3}}
                          <div class="carousel-item  img-fluid">
                               <img src="/upload/{{photos.pc3}}" alt="3">
                          </div>
                            {{/if}}
                            {{#if photos.pc4}}
                          <div class="carousel-item  img-fluid">
                               <img src="/upload/{{photos.pc4}}" alt="4">
                          </div>
                            {{/if}}
                            {{#if photos.pc5}}
                          <div class="carousel-item  img-fluid">
                               <img src="/upload/{{photos.pc5}}" alt="5">
                          </div>
                            {{/if}}
                            {{#if photos.pc6}}
                          <div class="carousel-item  img-fluid">
                               <img src="/upload/{{photos.pc6}}" alt="6">
                          </div>
                            {{/if}}
                        </div>
                        <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleIndicators{{id}}" data-bs-slide="prev">
                          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                          <span class="visually-hidden">Previous</span>
                        </button>
                        <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleIndicators{{id}}" data-bs-slide="next">
                          <span class="carousel-control-next-icon" aria-hidden="true"></span>
                          <span class="visually-hidden">Next</span>
                        </button>
                      </div>
                      </a>
                      <div class="name_job">{{firstname}}, {{age}}</div>
                      <p> {{work}}</p>
